	/**
	 * @param email - The Email Address to be validated
	 * @return true if the email address is valid, else return false.
	 * 
	 *         Uses the regular expression defined above to do the validation.
	 */
	public static boolean isValid(String email) {

		// https://mkyong.com/regular-expressions/how-to-validate-email-address-with-regular-expression/
		Pattern pattern;
		Matcher matcher;
		String EMAIL_PATTERN_CHATGPT = "^[\\w\\.-]+@[a-zA-Z\\d\\.-]+\\.[a-zA-Z]{2,}$";
		String EMAIL_PATTERN = "^[_A-Za-z0-9]+(\\.[_A-Za-z0-9]+)*@[A-Za-z0-9-]+(\\.[A-Za-z0-9]+)*(\\.[A-Za-z]{2,})$";
		String EMAIL_PATTERN_UsingRFC5322Regex = "^[a-zA-Z0-9_!#$%&'*+/=?`{|}~^.-]+@[a-zA-Z0-9.-]+$";
